{
    "common": [
        {
            "module": "../Common/MSBuildHelpers",
            "type": "ps"
        },
        {
            "module": "../Common/java-common",
            "type": "node",
            "compile": true
        }
    ],
    "rm": [
        {
            "items": [
                "node_modules/java-common/node_modules/azure-pipelines-task-lib"
            ],
            "options": "-Rf"
        }
    ],
    "externals": {
        "nugetv2": [
            {
                "name": "VstsTaskSdk",
                "version": "0.9.0",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                        "source": [
                            "*.dll",
                            "*.ps1",
                            "*.psd1",
                            "*.psm1",
                            "lib.json",
                            "Strings"
                        ],
                        "dest": "ps_modules/VstsTaskSdk/",
                        "options": "-R"
                    }
                ]
            }
        ]
    }
}
